{
  "name": "react-redux-datatable",
  "version": "1.1.3",
  "description": "Dynamic data table using React and Redux that fetches JSON from a predefined API.",
  "main": "./lib/index.js",
  "style": "./dist/styles.css",
  "dependencies": {
    "axios": "0.15.3",
    "react-bootstrap-table": "^3.2.2",
    "react-day-picker": "^5.2.3",
    "react-redux": "^5.0.2",
    "redux": "^3.6.0",
    "redux-promise-middleware": "^4.2.0",
    "redux-thunk": "^2.2.0",
    "moment": "^2.17.1",
    "url-search-params-polyfill": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "enzyme": "^2.7.1",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "history": "^3.2.1",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.2.1",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.5.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-overlays": "^0.6.11",
    "react-router": "^3.0.2",
    "style-loader": "^0.18.2",
    "webpack": "^2.4.1",
    "webpack-node-externals": "^1.5.4"
  },
  "peerDependencies": {
    "react": "^0.14.3 || ^15.0.0",
    "babel-polyfill": "^6.23.0"
  },
  "scripts": {
    "build": "npm run build:styles && npm run build:commonjs && npm run build:example && npm run build:umd && npm run build:umd:min",
    "build:commonjs": "BABEL_ENV=commonjs babel src --out-dir lib",
    "build:example": "webpack --config webpack.example.config.js --progress -p",
    "build:styles": "sass src/assets/sass/styles.scss:dist/styles.css --style compressed",
    "build:umd": "webpack --config webpack.umd.config.js --progress",
    "build:umd:min": "webpack --config webpack.umd.min.config.js --progress -p",
    "cover": "BABEL_ENV=test nyc --reporter=lcov --reporter=text-lcov mocha test/.setup.jsx './src/**/*.spec.jsx'",
    "coveralls": "npm run cover && cat ./coverage/lcov.info | coveralls",
    "prod": "NODE_ENV=production webpack --watch --progress -p",
    "test": "BABEL_ENV=dev mocha test/.setup.jsx './src/**/*.spec.jsx'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sean-ww/react-redux-datatable.git"
  },
  "keywords": [
    "react",
    "datatable",
    "data-table",
    "table",
    "grid",
    "react-component"
  ],
  "files": [
    "README.md",
    "lib/",
    "dist/"
  ],
  "author": "Sean Wallis <swwallis88@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sean-ww/react-redux-datatable/issues"
  },
  "homepage": "https://github.com/sean-ww/react-redux-datatable#readme"
}
